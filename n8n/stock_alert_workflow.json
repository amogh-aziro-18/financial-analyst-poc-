      "parameters": {
        "sendTo": "swathikiransr@gmail.com",
        "subject": "={{ \n  $('HTTP Request').item.json[\"alert\"][\"alert_status\"] === 'ALERT'\n    ? `ALERT â€“ ${$('HTTP Request').item.json[\"ticker\"]} price change ${$('HTTP Request').item.json[\"analysis\"][\"data\"][\"drop_percentage\"]}% over ${$('HTTP Request').item.json[\"price_data\"][\"data\"][\"period\"]}`\n    : `Status â€“ ${$('HTTP Request').item.json[\"ticker\"]} within range (${$('HTTP Request').item.json[\"analysis\"][\"data\"][\"drop_percentage\"]}% over ${$('HTTP Request').item.json[\"price_data\"][\"data\"][\"period\"]})`\n}}\n",
        "message": "=<div style=\"font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif; background-color:#f3f4f6; padding:20px 0;\">\n  <div style=\"max-width:700px; margin:0 auto; background-color:#ffffff; border-radius:8px; padding:24px; border:1px solid #e5e7eb;\">\n    \n    <!-- Header -->\n    <h2 style=\"margin:0 0 4px 0; font-size:22px; color:#111827;\">\n      Monitoring update for {{ $('HTTP Request').item.json[\"ticker\"] }} \n    </h2>\n    <p style=\"margin:0 0 16px 0; font-size:13px; color:#6b7280;\">\n      Automated alert generated by your Financial Analyst system.\n    </p>\n    \n    <hr style=\"border:none; border-top:1px solid #e5e7eb; margin:16px 0;\" />\n\n    <!-- Summary -->\n    <h3 style=\"margin:0 0 8px 0; font-size:16px; color:#111827;\">ðŸ“Œ Summary</h3>\n    <table style=\"width:100%; border-collapse:collapse; font-size:14px;\">\n      <tbody>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280; width:40%;\">Current price</td>\n          <td style=\"padding:6px 8px; color:#111827; font-weight:600;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"current_price\"] }}\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">Previous close</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"previous_price\"] }}\n          </td>\n        </tr>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280;\">Today's move</td>\n          <td style=\"padding:6px 8px; color:#b91c1c;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"change_pct\"] }}% \n            ({{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"change_amount\"] }})\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">Period analysed</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"period\"] }} \n            (interval: {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"interval\"] }})\n          </td>\n        </tr>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280;\">Cumulative move over period</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"analysis\"][\"data\"][\"drop_percentage\"] }}%\n          </td>\n        </tr>\n      </tbody>\n    </table>\n\n    <hr style=\"border:none; border-top:1px solid #e5e7eb; margin:16px 0;\" />\n\n    <!-- Alert Evaluation -->\n    <h3 style=\"margin:0 0 8px 0; font-size:16px; color:#111827;\">ðŸ“Œ Alert Evaluation</h3>\n    <table style=\"width:100%; border-collapse:collapse; font-size:14px;\">\n      <tbody>\n        <tr style=\"background-color:#fef2f2;\">\n          <td style=\"padding:6px 8px; color:#6b7280; width:40%;\">Alert status</td>\n          <td style=\"padding:6px 8px; color:#b91c1c; font-weight:700;\">\n            {{ $('HTTP Request').item.json[\"alert\"][\"alert_status\"] }}\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">Severity</td>\n          <td style=\"padding:6px 8px; color:#b91c1c; font-weight:600;\">\n            {{ $('HTTP Request').item.json[\"analysis\"][\"data\"][\"severity\"] }}\n          </td>\n        </tr>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280;\">System message</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"alert\"][\"message\"] }}\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">Threshold applied</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"alert\"][\"threshold\"] ?? $('HTTP Request').item.json[\"analysis\"][\"data\"][\"threshold\"] }}% drop\n          </td>\n        </tr>\n      </tbody>\n    </table>\n\n    <hr style=\"border:none; border-top:1px solid #e5e7eb; margin:16px 0;\" />\n\n    <!-- Market Context -->\n    <h3 style=\"margin:0 0 8px 0; font-size:16px; color:#111827;\">ðŸ“Œ Market Context</h3>\n    <table style=\"width:100%; border-collapse:collapse; font-size:14px;\">\n      <tbody>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280; width:40%;\">52-week high</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"52_week_high\"] }}\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">52-week low</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"52_week_low\"] }}\n          </td>\n        </tr>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280;\">Distance from high</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"distance_from_high\"] }}%\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">Distance from low</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"distance_from_low\"] }}%\n          </td>\n        </tr>\n        <tr style=\"background-color:#f9fafb;\">\n          <td style=\"padding:6px 8px; color:#6b7280;\">Current volume</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"volume\"] }}\n          </td>\n        </tr>\n        <tr>\n          <td style=\"padding:6px 8px; color:#6b7280;\">Average volume</td>\n          <td style=\"padding:6px 8px; color:#111827;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"avg_volume\"] }}\n          </td>\n        </tr>\n      </tbody>\n    </table>\n\n    <hr style=\"border:none; border-top:1px solid #e5e7eb; margin:16px 0;\" />\n\n    <!-- Recent Performance -->\n    <h3 style=\"margin:0 0 8px 0; font-size:16px; color:#111827;\">ðŸ“Œ Recent Daily Performance</h3>\n    <table style=\"width:100%; border-collapse:collapse; font-size:13px; border:1px solid #e5e7eb;\">\n      <thead>\n        <tr style=\"background-color:#f3f4f6;\">\n          <th style=\"padding:6px 8px; text-align:left; border-bottom:1px solid #e5e7eb;\">Date</th>\n          <th style=\"padding:6px 8px; text-align:right; border-bottom:1px solid #e5e7eb;\">Price</th>\n          <th style=\"padding:6px 8px; text-align:right; border-bottom:1px solid #e5e7eb;\">Return %</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for i in [0,1,2,3,4] %}\n        <tr style=\"background-color: {{ i % 2 == 0 ? '#ffffff' : '#f9fafb' }};\">\n          <td style=\"padding:6px 8px; text-align:left;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"historical_data\"][i][\"date\"] }}\n          </td>\n          <td style=\"padding:6px 8px; text-align:right;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"historical_data\"][i][\"price\"] }}\n          </td>\n          <td style=\"padding:6px 8px; text-align:right;\">\n            {{ $('HTTP Request').item.json[\"price_data\"][\"data\"][\"historical_data\"][i][\"return_pct\"] }}%\n          </td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n\n    <hr style=\"border:none; border-top:1px solid #e5e7eb; margin:16px 0;\" />\n\n    <!-- Analyst Note -->\n    <h3 style=\"margin:0 0 8px 0; font-size:16px; color:#111827;\">ðŸ“Œ Analyst Note</h3>\n    <div style=\"background-color:#f9fafb; border-radius:6px; padding:12px 14px; font-size:14px; color:#111827; line-height:1.5;\">\n      {{ $node[\"AI Agent\"].json[\"output\"] }}\n    </div>\n\n    <p style=\"margin-top:16px; font-size:11px; color:#9ca3af;\">\n      This alert was generated automatically based on your configured thresholds. \n      Review the information above before making any investment decisions.\n    </p>\n  </div>\n</div>\n",
